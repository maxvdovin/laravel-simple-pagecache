{
  "name": "maxvdovin/laravel-simple-pagecache",
  "description": "Laravel simple page cache module",
  "type": "library",
  "authors": [
    {
      "name": "Max Vdovin",
      "email": "maxvdovin@yandex.ru"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.3",
    "mockery/mockery": "^1.4.2"
  },
  "autoload": {
    "psr-4": {
      "MXJ\\PageCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MXJ\\PageCache\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MXJ\\PageCache\\PageCacheServiceProvider"
      ]
    }
  }
}
